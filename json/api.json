    },
    "host": "reqres.in",
    "basePath": "/api",
    "schemes": ["https"],
    "tags": [
      {
        "name": "Users"
      }
    ],
    "paths": {
      "/users": {
        "get": {
          "tags": ["Users"],
          "summary": "Ambil semua user",
          "produces": ["application/json"],
          "responses": {
            "200": {
              "description": "successful operation"
            }
          }
        },
        "post": {
          "tags": ["Users"],
          "summary": "Menambahkan user",
          "produces": ["application/json"],
          "operationId": "createUser",
          "parameters": [
            {
              "in": "body",
              "name": "body",
              "description": "Inputkan nama dan pekerjaan user baru",
              "required": true
            }
          ],
  
          "responses": {
            "200": {
              "description": "Successful operation"
            },
            "405": { "description": "Invalid input" }
          }
        }
      },
      "/users/2": {
        "get": {
          "tags": ["Users"],
          "summary": "Ambil data suatu user",
          "produces": ["application/json"],
          "responses": {
            "200": {
              "description": "successful operation"
            }
          }
        },
        "put": {
          "tags": ["Users"],
          "summary": "Memperbarui user yang ada",
          "parameters": [
            {
              "name": "body",
              "in": "body",
              "description": "name that need to be changed",
              "required": true
            }
          ],
          "responses": {
            "200": {
              "description": "Successful operation"
            },
            "400": { "description": "Invalid ID supplied" },
            "404": { "description": "User not found" },
            "405": { "description": "Validation exception" }
          }
        },
        "delete": {
          "tags": ["Users"],
          "summary": "Menghapus user",
          "responses": {
            "204": { "description": "No Content Success" }
          }
        }
      },
      "/users/23": {
        "get": {
          "tags": ["Users"],
          "summary": "Ambil user dengan id 23",
          "produces": ["application/json"],
          "responses": {
            "404": {
              "description": "User not found"
            }
          }
        }
      },
      "/unknown": {
        "get": {
          "tags": ["Users"],
          "summary": "Ambil seluruh data resource",
          "produces": ["application/json"],
          "responses": {
            "200": {
              "description": "successful operation"
            }
          }
        }
      }
    }
